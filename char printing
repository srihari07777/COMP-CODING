import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

class Hari {
    public static void main(String args[] ) throws Exception {

       int size =5;
       char ch='A';
       for(int i=0;i<size;i++)
       {
           for(int j=0;j<=i;j++)
           {
               System.out.print(ch+" ");
               ch++;
           }
           System.out.println("");
       }

    }
}
